{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "module": "es2020",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "ES2022",
    "types": ["node"],
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "ngx-soap": [
        "dist/ngx-soap"
      ],
      "ngx-soap/*": [
        "dist/ngx-soap/*"
      ],
      "@angular/*": [
        "./node_modules/@angular/*"
      ]
    },
    "useDefineForClassFields": false
  }
}
