{
  "name": "ngx-soap-next",
  "version": "0.12.0",
  "description": "SOAP service for Angular",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "typings": "src/index.d.ts",
  "moduleName": "ngx-soap",
  "scripts": {
    "transpile": "ngc",
    "bundle": "rimraf dist && npm run transpile && rollup -c rollup.config.umd.js",
    "postbundle": "node tools/bundle-scripts/remove-code.js",
    "build": "npm run bundle && npm run copy",
    "copy": "node tools/bundle-scripts/copy-files.js && node tools/bundle-scripts/cleanup.js",
    "test": "rimraf spec/out && tsc -p tsconfig.spec.json && jasmine",
    "build:lib": "ng build --configuration production ngx-soap && cp README.md dist/ngx-soap && cp LICENSE dist/ngx-soap",
    "pack": "cd ./dist/ngx-soap/ && npm pack && cd ../.. && cp dist/ngx-soap/ngx*tgz ..",
    "publish": "cd ./dist/ngx-soap/ && npm publish",
    "publish:dry-run": "cd ./dist/ngx-soap/ && npm publish --dry-run"
  },
  "keywords": [
    "Angular",
    "Angular10",
    "Angular11",
    "Angular12",
    "@angular",
    "SOAP",
    "WSDL"
  ],
  "author": "Luca Lulani, marcward, Oleg Abrazhaev",
  "license": "MIT",
  "readme": "README.md",
  "homepage": "https://github.com/seyfer/ngx-soap",
  "bugs": {
    "url": "https://github.com/seyfer/ngx-soap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seyfer/ngx-soap.git"
  },
  "dependencies": {
    "@angular/animations": "^12.2.17",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.17",
    "@angular/core": "^12.2.17",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "^12.2.17",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.17",
    "@angular/platform-browser-dynamic": "12.2.17",
    "@angular/router": "^12.2.17",
    "bluebird": "^3.7.2",
    "buffer": "^5.1.0",
    "concat-stream": "^1.6.2",
    "core-js": "^2.5.4",
    "crypto-js": "^3.1.9-1",
    "events": "^3.0.0",
    "httpntlm": "^1.7.6",
    "lodash": "^4.17.10",
    "ngx-soap-next": "^0.9.1",
    "rxjs": "^6.5.3",
    "sax": "^1.2.4",
    "stream": "0.0.2",
    "tslib": "^2.0.0",
    "uuid": "^3.3.2",
    "xml-crypto": "^1.5.3",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "^12.2.17",
    "@angular/language-service": "^12.2.17",
    "@types/estree": "^1.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.18.63",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "ng-packagr": "^12.2.7",
    "protractor": "~7.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
