{
  "name": "ngx-soap2",
  "version": "0.8.0",
  "description": "SOAP service for Angular4",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "typings": "src/index.d.ts",
  "moduleName": "ngx-soap",
  "scripts": {
    "transpile": "ngc",
    "bundle": "rimraf dist && npm run transpile && rollup -c rollup.config.umd.js",
    "postbundle": "node tools/bundle-scripts/remove-code.js",
    "build": "npm run bundle && npm run copy",
    "copy": "node tools/bundle-scripts/copy-files.js && node tools/bundle-scripts/cleanup.js",
    "test": "rimraf spec/out && tsc -p tsconfig.spec.json && jasmine",
    "build:lib": "ng build --configuration production ngx-soap && cp README.md dist/ngx-soap && cp LICENSE dist/ngx-soap",
    "pack": "cd ./dist/ngx-soap/ && npm pack && cd ../.. && cp dist/ngx-soap/ngx*tgz ..",
    "publish": "cd ./dist/ngx-soap/ && npm publish",
    "publish:dry-run": "cd ./dist/ngx-soap/ && npm publish --dry-run"
  },
  "keywords": [
    "Angular",
    "Angular2",
    "Angular4",
    "Angular5",
    "Angular6",
    "Angular7",
    "@angular",
    "SOAP",
    "WSDL"
  ],
  "author": "Luca Lulani, marcward, Oleg Abrazhaev",
  "license": "MIT",
  "readme": "README.md",
  "homepage": "https://github.com/seyfer/ngx-soap",
  "bugs": {
    "url": "https://github.com/seyfer/ngx-soap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seyfer/ngx-soap.git"
  },
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "^11.2.14",
    "buffer": "^5.1.0",
    "concat-stream": "^1.6.2",
    "core-js": "^2.5.4",
    "crypto-js": "^3.1.9-1",
    "events": "^3.0.0",
    "httpntlm": "^1.7.6",
    "lodash": "^4.17.10",
    "rxjs": "^6.6.3",
    "sax": "^1.2.4",
    "stream": "0.0.2",
    "tslib": "^2.0.0",
    "uuid": "^3.3.2",
    "xml-crypto": "^1.5.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular/cli": "~11.2.19",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  }
}
